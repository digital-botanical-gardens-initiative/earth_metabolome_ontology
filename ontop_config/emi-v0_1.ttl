                                            "https://orcid.org/0000-0003-3213-4495" ;
                        dcterms:description "The Earth Metabolome Initiative (EMI) vocabulary. EMI is a global effort to profile the metabolic content of all currently known species on our planet." ;
                        dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
                        dcterms:replaces <https://enpkg.commons-lab.org/kg> ;
                        dcterms:title "The Earth Metabolome semantic model." ;
                        vann:preferredNamespacePrefix "emi" ;
                        vann:preferredNamespaceUri "https://purl.org/emi#" ;
