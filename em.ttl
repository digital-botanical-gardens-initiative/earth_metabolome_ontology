                rdfs:subPropertyOf :hasPairMember ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :LFpair ;
                rdfs:range :LCMSFeature ;
                rdfs:comment "The first member of a spectral pair." ;
                rdfs:label "has first member" .


###  https://purl.org/emi#hasInChIKey
:hasInChIKey rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isInChIKeyOf ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Chemical
                                         :SpectrumAnnotation
                                       )
                         ] ;
             rdfs:range :InChIKey ;
             owl:propertyChainAxiom ( :hasChemicalStructure
                                      :isInChIKey2DOf
                                    ) ;
             rdfs:label "has InChIKey" .


